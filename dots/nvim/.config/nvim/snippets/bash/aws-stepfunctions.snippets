# aws.integration.SF (StepFunctions)
snippet aws-sfn-list
    aws stepfunctions list-state-machines \
        --max-items 1000 --query stateMachines[].name

snippet aws-sfn-list-arn
    aws stepfunctions list-state-machines \
        --query 'stateMachines[].stateMachineArn'

snippet aws-sfn-start
    aws stepfunctions start-execution \
        --state-machine-arn ${STATEMACHINE} \
        --name "Example exec $(uuidgen)" \
        --input '{"key": "value"}'
