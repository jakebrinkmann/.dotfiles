#!/usr/bin/env bash
#
# ‚ùØ _gitlab .cloud_deployment --env "RUNTIME_ENVIRONMENT=dev"

if [ -z ${@} ]; then
  grep -E "^\w+:[ -~]+:" .gitlab-ci.yml | sort
  exit 1;
else
  gitlab-runner exec docker \
    --docker-privileged \
    --docker-volumes /var/run/docker.sock:/var/run/docker.sock \
    --docker-pull-policy if-not-present \
    "${@}"
fi
