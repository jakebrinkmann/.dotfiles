// Place your key bindings in this file to overwrite the defaults
[
  { "key": "ctrl+pagedown", "command": "workbench.action.terminal.focusNext",
                               "when": "terminalFocus"   },
  { "key": "ctrl+pageup",   "command": "workbench.action.terminal.focusPrevious",
                               "when": "terminalFocus"   },
  { "key": "ctrl+shift+t",  "command": "workbench.action.terminal.new",
                               "when": "terminalFocus"  },
  { "key": "meta+ctrl+shift+alt+down",
                            "command": "editor.action.copyLinesDownAction",
                               "when": "editorTextFocus && !editorReadonly" },
  { "key": "meta+ctrl+shift+alt+up",
                            "command": "editor.action.copyLinesUpAction",
                               "when": "editorTextFocus && !editorReadonly" },
]
